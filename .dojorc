{
	"build-app": {
		"build-time-render": {
			"root": "root",
			"paths": [
				"home",
				"learn/routing/introduction",
				"learn/routing/basic-usage",
				"learn/routing/router-api",
				"learn/routing/route-configuration"
			],
			"puppeteerOptions": {
				"args": [
					"--no-sandbox",
					"--disable-setuid-sandbox"
				]
			}
		},
		"pwa": {
			"manifest": {
				"name": "dojo.io",
				"short_name": "dojo.io",
				"description": "dojo.io",
				"background_color": "#ffffff",
				"theme_color": "#222127",
				"icons": [
					{
						"src": "src/assets/favicon.ico",
						"sizes": [
							16
						]
					}
				]
			}
		}
	},
	"create-app": {}
}
